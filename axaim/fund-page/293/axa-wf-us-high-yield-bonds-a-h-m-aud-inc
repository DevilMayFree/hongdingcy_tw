    <!-- item filter -->
    <script>
    	$(function(){
    		$("#filter").on("change",function(){
    			var btn=$(this);
    			var sn=btn.val();
    			var url=$("#filter").find(":selected").data("url");
          location.href='https://www.cgsice.com/axaim/fund-page/'+sn+'/'+url;
    		})
    		$("#date_from,#date_to").on("change",function(){
    			let start_date=$("#date_from").val();
    			let end_date=$("#date_to").val();
    			let number=$("#number").val();
    			if(start_date && end_date)
    			{
    				$.post("https://www.cgsice.com/axaim/get_fund_net.php",{start_date:start_date,end_date:end_date,number:number},function(data){
    					d=JSON.parse(data);
    					$("#fund_container").html(d["fund_list"]);
    					$("#page_content").html(d["page_list"]);
    				})
    			}
    		})
    		$("body").on("click",".page_list",function(){
    			let btn=$(this);
    			let go_page=btn.data("page");
    			let page=$("#page").val();
    			if(page!=go_page)
    			{
    				let start_date=$("#date_from").val();
    				let end_date=$("#date_to").val();
    				let number=$("#number").val();
    				if(start_date && end_date)
    				{
    					$.post("https://www.cgsice.com/axaim/get_fund_net.php",{start_date:start_date,end_date:end_date,number:number,page:go_page},function(data){
    						d=JSON.parse(data);
    						$("#fund_container").html(d["fund_list"]);
    						$("#page_content").html(d["page_list"]);
    					})
    				}
    			}
    		})
    	})
    </script>

    <!-- time filter -->
    <script>
        let timeFrom = document.getElementById('date_from');
        let timeTo = document.getElementById('date_to');

        // current datetime
        let curDate = new Date();
        let curYear = curDate.getFullYear();
        let curMonth = curDate.getMonth() + 1;
        let curDay = curDate.getDate();

        if (curMonth < 10) {
            curMonth = "0" + curMonth;
        }
        if (curDay < 10) {
            curDay = "0" + curDay;
        }
        
        // earlier datetime ( current datetime minus 30 days )
        let earDate = new Date(curYear, curMonth - 1, curDay - 30);
        let earYear = earDate.getFullYear();
        let earMonth = earDate.getMonth() + 1;
        let earDay = earDate.getDate();

        if (earMonth < 10) {
            earMonth = "0" + earMonth;
        }
        if (earDay < 10) {
            earDay = "0" + earDay;
        }

        // time data format
        let currentDate = curYear + "-" + curMonth + "-" + curDay;
        let earlierDate = earYear + "-" + earMonth + "-" + earDay;
        
        // set initial value & placeholder
        // timeFrom.placeholder = earlierDate;
        // timeFrom.value = earlierDate;
        // timeTo.placeholder = currentDate;
        // timeTo.value = currentDate;

        // time filter trigger
        let curTimeStamp,
            earTimeStamp;

        function dateTimeUpdate() {
            let curDateUpdate = timeTo.value.split('-');
            let earDateUpdate = timeFrom.value.split('-');
            curTimeStamp = new Date(curDateUpdate[0], curDateUpdate[1] - 1, curDateUpdate[2]).getTime();
            earTimeStamp = new Date(earDateUpdate[0], earDateUpdate[1] - 1, earDateUpdate[2]).getTime();
        }

        timeFrom.onchange = function(e) {
            if (timeTo.value != null) {
                dateTimeUpdate();
                // if (curTimeStamp >= earTimeStamp) {
                //     alert('submit!');
                //     document.getElementById("dateFilter").submit();
                // } else {
                //     alert('wrong date input!')
                // }
            }
        }

        timeTo.onchange = function(e) {
            if (timeFrom.value != null) {
                dateTimeUpdate();
                // if (curTimeStamp >= earTimeStamp) {
                //     alert('submit!');
                //     document.getElementById("dateFilter").submit();
                // } else {
                //     alert('wrong date input!')
                // }
            }
        }
    </script>

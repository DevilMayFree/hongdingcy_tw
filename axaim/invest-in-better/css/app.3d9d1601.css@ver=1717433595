input[type=radio]:not(old):checked+label>span:before {
    content: "";
    display: block;
    width: 10px;
    height: 10px;
    margin: auto;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    position: absolute;
    background: #debe98
}

input[type=checkbox]:not(old):checked+label>span:before {
    border-radius: 0
}

input[type=file]:not(old) {
    width: 0;
    margin: 0;
    font-size: 0;
    opacity: 0;
    pointer-events: none;
    display: none
}

ul {
    list-style: none;
}
